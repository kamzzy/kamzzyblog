<div id="users">
<% if user_signed_in? %>
<%= button_to 'Sign Out', destroy_user_session_path, method: :delete %>
 <% @users.each do |user| %>
    <%= render user %>
    <% end %>
<% else %>
 <span class="sign-link"> <strong> <%= link_to 'Sign In', new_user_session_path %> </span>
 <span class="sign-link"> <%= link_to 'Sign Up', new_user_registration_path %></strong> </span>
<% end %>
</div>